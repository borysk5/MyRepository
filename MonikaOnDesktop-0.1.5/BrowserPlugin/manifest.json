{

    "manifest_version": 2,
    "name": "MonikaOnDesktop",
    "version": "1.0",
    "icons": {
        "96": "ico.png"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.6.0.min.js", "mod.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
}
